@model IEnumerable<Lab5_ED1.Models.Developer>

@{
    ViewBag.Title = "DevelopersList";
}

<h2>DevelopersList</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.User)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.User)
            </td>
            <td>
                @Html.ActionLink("Review Tasks", "DeveloperReview", new { id = item.Id })
            </td>
        </tr>
    }

</table>
